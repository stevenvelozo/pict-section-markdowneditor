{
	"name": "book_viewer_example",
	"version": "1.0.0",
	"description": "Lorem ipsum book rendered with pict-section-content, editable per section",
	"main": "BookViewer-Example-Application.js",
	"scripts": {
		"start": "node BookViewer-Example-Application.js",
		"build": "npx quack build && npx quack copy"
	},
	"author": "steven",
	"license": "MIT",
	"copyFilesSettings": {
		"whenFileExists": "overwrite"
	},
	"copyFiles": [
		{
			"from": "./html/*",
			"to": "./dist/"

		{
			"from": "../../node_modules/pict/dist/*",
			"to": "./dist/"
		}
	],
	"dependencies": {
		"pict": "^1.0.350",
		"pict-application": "^1.0.19",
		"pict-view": "^1.0.66"
	},
	"devDependencies": {
		"@codemirror/lang-markdown": "^6.5.0",
		"@codemirror/state": "^6.5.4",
		"codemirror": "^6.0.2",
		"esbuild": "^0.27.3"
	}
}